{"ast":null,"code":"export default {\n  data() {\n    return {\n      currentIndex: 0,\n      intervalId: null\n    };\n  },\n\n  computed: {\n    pages() {\n      return new Array(4).fill(null);\n    }\n\n  },\n\n  mounted() {\n    this.initSlider();\n  },\n\n  methods: {\n    initSlider() {\n      this.intervalId = setInterval(this.nextSlide, 2000);\n    },\n\n    nextSlide() {\n      this.currentIndex = (this.currentIndex + 1) % 4;\n    },\n\n    prevSlide() {\n      this.currentIndex = (this.currentIndex - 1 + 4) % 4;\n    },\n\n    handlePaginationClick(event) {\n      if (event.target.id.startsWith('pagenation-item')) {\n        const index = Number(event.target.id.slice(-1));\n        this.currentIndex = index;\n      }\n    },\n\n    stopAutoSlide() {\n      clearInterval(this.intervalId);\n    },\n\n    startAutoSlide() {\n      this.intervalId = setInterval(this.nextSlide, 2000);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAiBA,eAAe;EACbA,IAAI,GAAG;IACL,OAAO;MACLC,YAAY,EAAE,CADT;MAELC,UAAU,EAAE;IAFP,CAAP;EAID,CANY;;EAObC,QAAQ,EAAE;IACRC,KAAK,GAAG;MACN,OAAO,IAAIC,KAAJ,CAAU,CAAV,EAAaC,IAAb,CAAkB,IAAlB,CAAP;IACF;;EAHQ,CAPG;;EAYbC,OAAO,GAAG;IACR,KAAKC,UAAL;EACD,CAdY;;EAebC,OAAO,EAAE;IACPD,UAAU,GAAG;MACX,KAAKN,UAAL,GAAkBQ,WAAW,CAAC,KAAKC,SAAN,EAAiB,IAAjB,CAA7B;IACD,CAHM;;IAIPA,SAAS,GAAG;MACV,KAAKV,YAAL,GAAoB,CAAC,KAAKA,YAAL,GAAoB,CAArB,IAA0B,CAA9C;IACD,CANM;;IAOPW,SAAS,GAAG;MACV,KAAKX,YAAL,GAAoB,CAAC,KAAKA,YAAL,GAAoB,CAApB,GAAwB,CAAzB,IAA8B,CAAlD;IACD,CATM;;IAUPY,qBAAqB,CAACC,KAAD,EAAQ;MAC3B,IAAIA,KAAK,CAACC,MAAN,CAAaC,EAAb,CAAgBC,UAAhB,CAA2B,iBAA3B,CAAJ,EAAmD;QACjD,MAAMC,KAAI,GAAIC,MAAM,CAACL,KAAK,CAACC,MAAN,CAAaC,EAAb,CAAgBI,KAAhB,CAAsB,CAAC,CAAvB,CAAD,CAApB;QACA,KAAKnB,YAAL,GAAoBiB,KAApB;MACF;IACD,CAfM;;IAgBPG,aAAa,GAAG;MACdC,aAAa,CAAC,KAAKpB,UAAN,CAAb;IACD,CAlBM;;IAmBPqB,cAAc,GAAG;MACf,KAAKrB,UAAL,GAAkBQ,WAAW,CAAC,KAAKC,SAAN,EAAiB,IAAjB,CAA7B;IACF;;EArBO;AAfI,CAAf","names":["data","currentIndex","intervalId","computed","pages","Array","fill","mounted